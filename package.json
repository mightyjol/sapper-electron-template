  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently \"sapper dev\" \"wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .\"",
    "dev-web": "sapper dev",
    "build": "sapper build",
    "releasewin": "cmd /c release.sh",
    "start": "electron .",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "package": "yarn build && electron-packager . pos --overwrite --all --icon=static/icons/win/icon.ico --prune=true --out=dist --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"POS application\"",
    "setup": "electron-installer-windows --src dist/pos-win32-x64/ --dest dist/installers/"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "electron": "^7.1.2",
    "electron-settings": "^3.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "node-cmd": "^3.0.0",
    "shelljs": "^0.8.3",
    "uuid": "^3.3.3",
    "lowdb": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.7.1",
    "@babel/runtime": "^7.7.2",
    "bulma": "^0.7.5",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "electron-packager": "^14.2.1",
    "faunadb": "^2.9.4",
    "lowdb": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.27.4",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-multi-input": "^1.0.3",
    "rollup-plugin-node-resolve": "5.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.12",
    "sirv": "^0.4.2",
    "svelte": "^3.15.0",
